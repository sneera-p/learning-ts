{
  "name": "express",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint src/**/.ts",
    "dev": "concurrently \"webpack -c .config/webpack/webpack.dev.js --watch\" \"nodemon -e \"ts, js\" -w ./dist/ -V -x node dist/index.js\"",
    "start": "bash scripts/docker.sh 1.0 dev",
    "serve": "bash scripts/docker.sh 1.0 prod",
    "clean": "bash scripts/clean.sh",
    "test": "jest",
    "test:unit": "jest --config=.config/jest/jest.unit.js",
    "test:int": "jest --config=.config/jest/jest.int.js"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.3.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "pkg": "^5.8.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0",
    "yaml": "^2.1.1"
  }
}
